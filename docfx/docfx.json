{
  "__metadata": [
    {
      "src": [
        {
          "src": "../NF.Tool.ExcelFlow/",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "allowCompilationErrors": true,
      "filter": "filterConfig.yml",
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/res/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "NF.Tool.ExcelFlow",
      "_appTitle": "NF.Tool.ExcelFlow",
	  "_googleAnalyticsTagId": "G-855BJNV9VD",
      "_enableSearch": true,
      "pdf": false
    }
  }
}